<script>
function setTotal() {
    
    $('#pdesc').val("");
    var grandTotal = 0;
    if($('#prd1').val()=='1') {
        grandTotal = grandTotal + 25 * parseInt($("#q1").val());
        if(parseInt($("#q1").val()) > 0)
            $('#pdesc').val( $('#pdesc').val() + 'כרטיס בוגר ליום אחד - 25 ש"ח * ' + $("#q1").val() + ',');
    }
    if($('#prd2').val()=='1') {
        grandTotal = grandTotal + 35 * parseInt($("#q2").val());
         if(parseInt($("#q2").val()) > 0)
            $('#pdesc').val( $('#pdesc').val() + 'כרטיס בוגר ליומיים - 35 ש"ח * ' + $("#q2").val() + ',');
    }
    if($('#prd3').val()=='1') {
        grandTotal = grandTotal + 20 * parseInt($("#q3").val());
         if(parseInt($("#q3").val()) > 0)
            $('#pdesc').val(  $('#pdesc').val() + 'כרטיס נוער ליום אחד - 20 ש"ח * ' + $("#q3").val() + ',');
    }
    if($('#prd4').val()=='1') {
        grandTotal = grandTotal + 25 * parseInt($("#q4").val());
         if(parseInt($("#q4").val()) > 0)
            $('#pdesc').val($('#pdesc').val() + 'כרטיס נוער ליומיים - 25 ש"ח * ' + $("#q4").val() + ',');
    }
    
    
    $('#grandTotalTxt').html(grandTotal);
    $('#sum').val(grandTotal);
    
}

function submitForm() {
    $('#contact').val($('#firstName').val() + " " + $('#lastName').val()+ " " + $('#identity').val());
    if($('#firstName').val()=='') {
        alert ("חובה להקליד שם פרטי");
        $('#firstName').focus();
        return false;
    }
    if($('#lastName').val()=='') {
        alert ("חובה להקליד שם פרטי");
        $('#lastName').focus();
        return false;
    }
    if($('#phone').val()=='') {
        alert ("חובה לעדכן את מספר הטלפון");
        $('#phone').focus();
        return false;
    }
    
    if($('#email').val()=='') {
        alert ("חובה לעדכן את האימייל");
        $('#email').focus();
        return false;
    }  
    
   if(!validateEmail($('#email').val())) {
        alert ("שדה האימייל לא הוקלד תקין");
        $('#email').focus();
        return false;
    }      
      
    
    document.getElementById('reg').submit();
}

function validateEmail(email) {
    var re = /^(([^<>()\[\]\\.,;:\s@"]+(\.[^<>()\[\]\\.,;:\s@"]+)*)|(".+"))@((\[[0-9]{1,3}\.[0-9]{1,3}\.[0-9]{1,3}\.[0-9]{1,3}])|(([a-zA-Z\-0-9]+\.)+[a-zA-Z]{2,}))$/;
    return re.test(email);
}
